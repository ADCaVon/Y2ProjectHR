@using HREFEntity;
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>My JSP 'position-release_details.jsp' starting page</title>

    <link rel="stylesheet"
          href="../../../Content/table.css" type="text/css">
    <link rel="stylesheet"
          href="../../../Content/cwcalendar.css" type="text/css">
    <script type="text/javascript"
            src="../../../javascript/comm/comm.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/comm/list.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/calendar-ch.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/jquery-1.7.2.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/locate.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/select.js">
    </script>
    <script src="~/Scripts/vue.js"></script>
</head>
<body>
    @{engage_major_release emr = ViewBag.emr;}
    <form name="humanfileForm" method="get" action="/recruit/register/@emr.mre_id">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">您正在做的业务是：人力资源--招聘管理--职位发布管理--职位发布查询--详细  </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="确认申请" class="BUTTON_STYLE1">
                    <input type="reset" value="返回" class="BUTTON_STYLE1">
                </td>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td class="TD_STYLE1" width="11%">
                    I级机构
                </td>
                <td width="14%" class="TD_STYLE2">

                 
                        <input type="text" value="@emr.first_kind_name">
                </td>
                <td width="11%" class="TD_STYLE1">
                    II级机构
                </td>
                <td width="14%" class="TD_STYLE2">
         
                    <input type="text" value="@emr.second_kind_name">
                </td>
                <td width="11%" class="TD_STYLE1">
                    III级机构
                </td>
                <td class="TD_STYLE2">
                 
                    <input type="text" value="@emr.third_kind_name">
                </td>
                <td width="11%" class="TD_STYLE1">
                    招聘类型
                </td>
                <td class="TD_STYLE2" colspan="2">
             
                    <input type="text" value="@emr.engage_type">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    职位分类
                </td>
                <td class="TD_STYLE2">
       
                    <input type="text" value="@emr.major_kind_name">
                </td>
                <td class="TD_STYLE1">
                    职位名称
                </td>
                <td class="TD_STYLE2">

                    <input type="text" value="@emr.major_name">
                </td>
                <td class="TD_STYLE1">
                    招聘人数
                </td>
                <td class="TD_STYLE2">
                    <input type="text" value="@emr.human_amount" name="human_amount" id="humanAmount" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    截止日期
                </td>
                <td class="TD_STYLE2">
                    <input value="@emr.deadline" type="text" name="deadline" readonly
                           class="INPUT_STYLE2" id="date_start">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    登记人
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="register" value="@emr.register" class="INPUT_STYLE2">
                </td>
                <td class="TD_STYLE1">
                    登记时间
                </td>
                <td class="TD_STYLE2">

                    <input type="text" name="regist_time"
                           id="nowTime" readonly="readonly"
                           class="INPUT_STYLE2" value="@emr.regist_time">
                </td>

                <td>&nbsp; </td>
                <td>&nbsp; </td>
                <td>&nbsp; </td>
                <td><input type="hidden" name="mre_id" value="@emr.mre_id" /></td>


            </tr>
            <tr>
                <td class="TD_STYLE1">
                    职位描述
                </td>
                <td class="TD_STYLE2" colspan="8">
                    <textarea name="major_describe" rows="4" cols="80" class="TEXTAREA_STYLE1">@emr.major_describe</textarea>
                </td>

            </tr>
            <tr>
                <td class="TD_STYLE1">
                    招聘要求
                </td>
                <td class="TD_STYLE2" colspan="8">
                    <textarea name="engage_required" rows="4" cols="80" class="TEXTAREA_STYLE1">@emr.engage_required</textarea>
                </td>

            </tr>
        </table>
    </form>
</body>

</html>
<script type="text/javascript">
        $("select").attr({
            "class": "SELECT_STYLE1"
        })
        $("input[type=text]").attr({
            "class": "INPUT_STYLE2",
            "readonly":"readonly"
           

        })
        $("input[type=datetime]").attr({
            "class": "INPUT_STYLE2",
             "readonly":"readonly"

        })
        $("textarea").attr({
            "class": "TEXTAREA_STYLE1",
            "rows": "4",
             "readonly":"readonly"
        })
</script>