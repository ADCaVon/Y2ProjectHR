@model IEnumerable<HRModel.M_human_file>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>query_list_information</title>
    <link rel="stylesheet"
          href="../../Content/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/jquery-1.7.2.js">
    </script>
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
    <script language="javascript"
            src="../../javascript/winopen/winopenm.js">
    </script>
</head>
<body>
    <table width="100%">
        <tr>
            <td>
                <font color="black">您正在做的业务是：人力资源--人力资源档案管理--人力资源档案查看 </font>
            </td>
        </tr>
        <tr>
            <td align="right">
                <input type="button" value="返回" class="BUTTON_STYLE1"
                       onclick="history.back()">
            </td>
        </tr>
    </table>
    @foreach (var item in Model)
    {
        <table width="100%" border="1" cellpadding=3 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td class="TD_STYLE1" width="10%">
                    档案编号
                </td>
                <td colspan="6" class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_id)
                </td>
                <td rowspan="6" width="13%" style="text-align: center;">
                    <img src="../../Uploader/@(item.human_picture).jpg" style="width:120px;height:150px;" />
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1" width="10%">
                    I级机构
                </td>
                <td width="13%" class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.first_kind_name)
                </td>
                <td width="10%" class="TD_STYLE1">
                    II级机构
                </td>
                <td width="13%" class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.second_kind_name)
                </td>
                <td width="10%" class="TD_STYLE1">
                    III级机构
                </td>
                <td class="TD_STYLE2" colspan="2" width="2%">
                    @Html.TextBoxFor(modelItem => item.third_kind_name)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    职位分类
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_major_kind_name)
                </td>
                <td class="TD_STYLE1">
                    职位名称
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_major_name)
                </td>
                <td class="TD_STYLE1">
                    职称
                </td>
                <td colspan="2" class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_pro_designation)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    姓名
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_name)
                </td>
                <td class="TD_STYLE1">
                    性别
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_sex)
                </td>
                <td class="TD_STYLE1">
                    EMAIL
                </td>
                <td colspan="2" class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_email)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    电话
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_telephone)
                </td>
                <td class="TD_STYLE1">
                    QQ
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_qq)
                </td>
                <td class="TD_STYLE1">
                    手机
                </td>
                <td colspan="2" class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_mobilephone)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    住址
                </td>
                <td colspan="3" class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_address)
                </td>
                <td class="TD_STYLE1">
                    邮编
                </td>
                <td colspan="2" class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_postcode)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    国籍
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_nationality)
                </td>
                <td class="TD_STYLE1">
                    出生地
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_birthplace)
                </td>
                <td class="TD_STYLE1">
                    生日
                </td>
                <td width="13%" class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_birthday)
                </td>
                <td width="10%" class="TD_STYLE1">
                    民族
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_race)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    宗教信仰
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_religion)
                </td>
                <td class="TD_STYLE1">
                    政治面貌
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_party)
                </td>
                <td class="TD_STYLE1">
                    身份证号码
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_id_card)
                </td>
                <td class="TD_STYLE1">
                    社会保障号码
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_society_security_id)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    年龄
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_age)
                </td>
                <td class="TD_STYLE1">
                    学历
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_educated_degree)
                </td>
                <td class="TD_STYLE1">
                    教育年限
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_educated_years)
                </td>
                <td class="TD_STYLE1">
                    学历专业
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_educated_major)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    薪酬标准
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.salary_standard_name)
                </td>
                <td class="TD_STYLE1">
                    开户行
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_bank)
                </td>
                <td class="TD_STYLE1">
                    帐号
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_account)
                </td>
                <td class="TD_STYLE1">
                    复核人
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.checker)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    复核时间
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.check_time)
                </td>
                <td class="TD_STYLE1">
                    特长
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_speciality)
                </td>
                <td class="TD_STYLE1">
                    爱好
                </td>
                <td class="TD_STYLE2">
                    @Html.TextBoxFor(modelItem => item.human_hobby)
                </td>
                <td class="TD_STYLE1">
                    &nbsp;
                </td>
                <td class="TD_STYLE2">
                    &nbsp;
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    档案附件
                </td>
                <td colspan="7" class="TD_STYLE2">
                    <a href="#"></a>
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    个人履历
                </td>
                <td colspan="7" class="TD_STYLE2">
                    @Html.TextAreaFor(modelItem => item.human_histroy_records)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    家庭关系信息
                </td>
                <td colspan="7" class="TD_STYLE2">
                    @Html.TextAreaFor(modelItem => item.human_family_membership)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    备注
                </td>
                <td colspan="7" class="TD_STYLE2">
                    @Html.TextAreaFor(modelItem => item.remark)
                </td>
            </tr>
        </table>
    }
    <script type="text/javascript">
        $("select").attr({
            "class": "SELECT_STYLE1"
        })
        $("input[type=text]").attr({
            "class": "INPUT_STYLE2",
            "readOnly":"true"
        })
        $("textarea").attr({
            "class": "TEXTAREA_STYLE1",
            "rows": "4",
            "readOnly":"true"
        })

    </script>
</body>
</html>
